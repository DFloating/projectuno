<h1>Journals#new</h1>
<p>Find me in app/views/journals/new.html.erb</p>

<form action="<%= journals_index_path %>" method="post">
    <label>
        Title: 
        <input name="name" required>
    </label>
    
    <label>
        Photo:
        <input type="url" name="image">
    </label>

    <label>
        Date:
        <input type="number" name="orbit">
    </label>

    <label>
        Story:
        <input type="number" name="diameter">
    </label>


    <button>Create journal entry</button>
</form>